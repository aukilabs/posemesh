{
    "presets": [
        ["@babel/preset-env", {
            "corejs": 3,
            "modules": false,
            "targets": "> 0.25%, not dead",
            "useBuiltIns": "usage"
        }],
        ["minify"]
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime", {
            "corejs": 3,
            "regenerator": true
        }]
    ]
}
